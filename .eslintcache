         {props.ingredients && <BuildControls\r\n                addIngredients={props.addIngredients}\r\n                removeIngredients={props.removeIngredients}\r\n                disabled={disabledInfo}\r\n                purchase={purchaseHandler()}\r\n                price={props.totalPrice}\r\n                handleClick={showModalHandler}\r\n            />}\r\n        </React.Fragment>\r\n    );\r\n}\r\n\r\nconst mapStateToProps = (state) => {\r\n    return {\r\n        ingredients: state.ingredients,\r\n        totalPrice: state.totalPrice\r\n    };\r\n}\r\n\r\nconst mapDispatchToProps = (dispatch) => {\r\n    return {\r\n        addIngredients: (ingName) => {\r\n            dispatch({\r\n                type: actionTypes.ADD_INGREDIENT,\r\n                ingredientName: ingName\r\n            })\r\n        },\r\n        removeIngredients: (ingName) => dispatch({\r\n            type: actionTypes.REMOVE_INGREDIENT,\r\n            ingredientName: ingName\r\n        })\r\n    };\r\n}\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(BurgerBuilder);\r\n",["123","124"],"C:\\Users\\DELL\\Documents\\Web Development\\burger-builder\\src\\components\\Burger\\Burger.js",[],"C:\\Users\\DELL\\Documents\\Web Development\\burger-builder\\src\\components\\Burger\\Ingredients.js",[],"C:\\Users\\DELL\\Documents\\Web Development\\burger-builder\\src\\components\\Controllers\\BuildControls.js",[],"C:\\Users\\DELL\\Documents\\Web Development\\burger-builder\\src\\components\\Controllers\\SingleController.js",[],"C:\\Users\\DELL\\Documents\\Web Development\\burger-builder\\src\\components\\Burger\\Summary.js",[],"C:\\Users\\DELL\\Documents\\Web Development\\burger-builder\\src\\components\\UI\\Backdrop.js",[],"C:\\Users\\DELL\\Documents\\Web Development\\burger-builder\\src\\components\\Layout\\Layout.js",[],"C:\\Users\\DELL\\Documents\\Web Development\\burger-builder\\src\\components\\Navigation\\Logo.js",[],"C:\\Users\\DELL\\Documents\\Web Development\\bur